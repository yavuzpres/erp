<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sale_order_mass_import_form_pricelist" model="ir.ui.view">
        <field name="name">sale.order.mass.import.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale_product_multi_add.sale_order_mass_import_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='%(action_import_product_to_sale)d']" position="attributes">
                <attribute name="context">{'pricelist_id': id}</attribute>
            </xpath>
        </field>
    </record>
</odoo>
